{
  "name": "@mfkn/fkn-web",
  "private": true,
  "version": "0.0.4",
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@apollo/server": "^4.7.4",
    "@banou/asar": "^0.0.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "apollo-upload-client": "^17.0.0",
    "base-x": "^4.0.0",
    "dexie": "^3.2.4",
    "dompurify": "^3.0.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "marked": "^4.2.12",
    "osra": "^0.0.10",
    "parse-torrent": "^11.0.7",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-flip-toolkit": "^7.0.17",
    "react-hook-form": "^7.43.4",
    "react-image-crop": "^10.0.9",
    "react-router-dom": "^6.8.2",
    "rxdb": "^14.13.0",
    "rxjs": "^7.8.1",
    "slate": "^0.91.4",
    "slate-history": "^0.86.0",
    "slate-react": "^0.91.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/client-preset": "^2.1.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^3.1.0",
    "@rollup/plugin-inject": "^5.0.3",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "assert": "^2.0.0",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "node-stdlib-browser": "^1.2.0",
    "nodemon": "^2.0.21",
    "parse-torrent-file": "^4.2.2",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "scripts": {
    "copy-wellknown": "copyfiles -u 2 ./src/.well-known/* build/.well-known",
    "copy-html": "copyfiles -u 1 ./src/index.html build",
    "copy-libass": "copyfiles -u 4 ./node_modules/libass-wasm/dist/js/* build",
    "build-index": "node ./build.js",
    "build-index-watch": "node ./build.js -w -s",
    "type-check-index": "tsc",
    "type-check-index-watch": "tsc --watch",
    "build-sw": "esbuild ./src/service-worker.ts --format=esm --bundle --outfile=build/service-worker.js",
    "build-sw-watch": "esbuild --watch ./src/service-worker.ts --format=esm --bundle --outfile=build/service-worker.js",
    "type-check-sw": "tsc --p ./src/service-worker/tsconfig.json",
    "type-check-sw-watch": "tsc --watch --p ./src/service-worker/tsconfig.json",
    "build": "vite build",
    "build2": "npm run copy-html && npm run build-index && npm run build-sw && npm run type-check-index && npm run type-check-sw",
    "watch": "concurrently \"npm run build-index-watch\" \"npm run build-sw-watch\" \"npm run type-check-index-watch\" \"npm run type-check-sw-watch\"",
    "dev": "concurrently \"vite --port 1234\" \"npm run codegen-watch\"",
    "codegen-watch": "nodemon -e graphql,ts,tsx --watch src/local-only.graphql --watch src/schema.ts --exec \"npm run codegen\"",
    "codegen": "node _codegen.mjs && graphql-codegen --config codegen.ts & node _codegen.js",
    "_codegen": "npm run export-typedefs && node _codegen.mjs && graphql-codegen --config codegen.ts & node _codegen.js",
    "export-typedefs": "vite -c typedefs-vite.config.ts build && node temp/typedefs.js && shx rm -rf temp",
    "_codegen-watch": "node _codegen.mjs && graphql-codegen -w --config codegen.ts & node _codegen.js"
  },
  "type": "module"
}
